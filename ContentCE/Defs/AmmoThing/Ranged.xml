	<RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_MakaiStarParticleVariaBow</defName>
    <label>make star particle energy arrow x200</label>
    <description>Craft 200 star particle ammo cartridges.</description>
    <jobString>Making Star Particle ammo cartridges.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Makai_Stardust</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Makai_Stardust</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_MakaiStarParticleVariaBow>200</Ammo_MakaiStarParticleVariaBow>
    </products>
    <workAmount>6000</workAmount>
  </RecipeDef>
  
