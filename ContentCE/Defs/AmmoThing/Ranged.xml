	<ThingDef Class="CombatExtended.AmmoDef" Name="StarParticleBase" ParentName="AmmoBase" Abstract="True">
    <description>Special ammo used by Makai weaponary.</description>
    <statBases>
      <MarketValue>50</MarketValue>
	  <Mass>0.1</Mass>
	  <Bulk>0.1</Bulk>
	  <MaxHitPoints>1000</MaxHitPoints>
    </statBases>
    <tradeTags>
      <li>CE_AutoEnableTrade</li>
      <li>CE_AutoEnableCrafting_Makai_Bench</li>
    </tradeTags>
    <thingCategories>
      <li>Makai_StarParticleAmmo</li>
    </thingCategories>
        <stackLimit>200</stackLimit>
	    <cookOffFlashScale>40</cookOffFlashScale>
    <cookOffSound>MortarBomb_Explode</cookOffSound>
    <ammoClass>Makai_StarParticleAmmoClass</ammoClass>
  </ThingDef>
  
