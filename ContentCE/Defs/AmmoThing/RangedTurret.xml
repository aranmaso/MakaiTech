	<!-- RecipeDef -->
	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeAmmo_MakaiStarParticleEnergyTurret</defName>
		<label>make compressed-stardust cartridges x250</label>
		<description>Craft 250 star particle ammo cartridges.</description>
		<jobString>Making Star Particle ammo cartridges.</jobString>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>Makai_Stardust</li>
			  </thingDefs>
			</filter>
			<count>25</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>Makai_Stardust</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <Ammo_MakaiStarParticleEnergyTurret>250</Ammo_MakaiStarParticleEnergyTurret>
		</products>
		<workAmount>6000</workAmount>
	</RecipeDef>
	
