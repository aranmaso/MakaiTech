<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<XmlExtensions.SettingsMenuDef>
	<defName>Makai_Tab_1</defName>
	<modId>Aranmaho.Makai</modId>
	<settings>
		<li Class="XmlExtensions.Setting.Gap"/>
		<li Class="XmlExtensions.Setting.GapLine" />
		<li Class="XmlExtensions.Setting.SetColor">
		<color>(95,87,165)</color>
		</li>
        <li Class="XmlExtensions.Setting.Text">
            <text>MakaiTech</text>
            <anchor>Middle</anchor>
            <font>Medium</font>
        </li>
		<li Class="XmlExtensions.Setting.SetColor"/>
        <li Class="XmlExtensions.Setting.GapLine"/>
        <li Class="XmlExtensions.Setting.Checkbox">
            <label>change Celestial Steel to Makainium?</label>
            <key>Makai_Ingot_Oldname</key>
            <defaultValue>false</defaultValue>
        </li>
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>unbalanced Stat value for celestial steel?</label>
			<key>Makai_Ingot_OP</key>
			<defaultValue>false</defaultValue>
		</li>
		<!--li Class="XmlExtensions.Setting.Checkbox">
			<label>no oversized weapon</label>
			<key>Makai_Weapon_NoBig</key>
			<defaultValue>false</defaultValue>
		</li-->
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>stardust is universal fuel</label>
			<key>Stardust.UniversalFuel</key>
			<defaultValue>true</defaultValue>
		</li>
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>Flame of the First Forge is universal fuel</label>
			<key>ForgeFlame.UniversalFuel</key>
			<defaultValue>true</defaultValue>
		</li>
		<!--li Class="XmlExtensions.Setting.Checkbox" MayRequire="VanillaExpanded.VWEL">
			<label>entity null has ranged attack</label>
			<key>Makai_Melee_hasRange</key>
			<defaultValue>false</defaultValue>
		</li-->
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>Celestial Floor don't have big sun in it.</label>
			<key>Makai_CelestF_noSun</key>
			<defaultValue>true</defaultValue>
		</li>
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>Celestial Floor don't have big sun and line in it.</label>
			<key>Makai_CelestF_noSun_noLine</key>
			<defaultValue>false</defaultValue>
		</li>
		<li Class="XmlExtensions.Setting.Checkbox">
			<label>Celestial Steel can spawn on random as stuffable(0.05 commonality)</label>
			<key>Makai_Ingot_Commonality_Toggle</key>
			<defaultValue>false</defaultValue>
		</li>
		<li Class="XmlExtensions.Setting.SetColor">
				<color>(255,0,0)</color>
			</li>
			<li Class="XmlExtensions.Setting.Text">
				<text>Restarting game is required to apply any changes.</text>
				<tKey>Global_InfoTextAboutRestart</tKey>
			</li>
		<li Class="XmlExtensions.Setting.SetColor"/>
		<li Class="XmlExtensions.Setting.ResetSettings">
				<label>Reset All Settings</label>
				<tKey>Global_ResetAllSettings</tKey>
		</li>
	</settings>

</XmlExtensions.SettingsMenuDef>

</Defs>