				<li>Makai_Essence6_Bionic</li>
		</researchPrerequisites>
		<skillRequirements>
			<Medicine>15</Medicine>
		</skillRequirements>
		<recipeUsers>
			<li>Human</li>
		</recipeUsers>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>Makai_Dis-Path_E</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Makai_Dis-Path_E</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Shoulder</li>
		</appliedOnFixedBodyParts>
		<products>
			<Makai_Dis-Path_E>1</Makai_Dis-Path_E>
		</products>
	</RecipeDef>

