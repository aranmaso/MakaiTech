			<li>Human</li>
		</recipeUsers>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>Makai_Dis-Path_E</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
