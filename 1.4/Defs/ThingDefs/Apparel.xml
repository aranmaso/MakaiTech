    <graphicData>
      <texPath>Things/Apparel/AnvaRing/anva_ring</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <drawSize>1.2</drawSize>
    </graphicData>
	<tickerType>Normal</tickerType>
	<verbs>
		<li>
			<label>zappshot</label>
			<verbClass>Verb_Shoot</verbClass>
			<hasStandardCommand>true</hasStandardCommand>
			<defaultProjectile>Makai_Lightning_Ball</defaultProjectile>
			<warmupTime>1</warmupTime>
			<range>30</range>
			<minRange>2</minRange>
